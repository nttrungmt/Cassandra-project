public class Payment{
	int C_W_ID, C_D_ID, C_ID, paymentAmount;
	
	Payment(C_W_ID, C_D_ID, C_ID, paymentAmount)
	{
		//set the parameters;
		this.C_W_ID = C_W_ID;
		this.C_D_ID = C_D_ID;
		this.C_ID = C_ID;
		this.paymentAmount = paymentAmount;
	}
}